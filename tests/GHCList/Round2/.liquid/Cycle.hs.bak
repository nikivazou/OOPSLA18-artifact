{-# LANGUAGE CPP #-}
{-@ LIQUID "--no-termination" @-}

module Round2.Cycle where

import GHCListRound1 hiding (cycle)
import Prelude       hiding (cycle)

{-@ cycle               :: xs:{[a] | ?? } -> [a] @-}
cycle                   :: [a] -> [a]
cycle []                = errorEmptyList "cycle"
cycle xs                = xs' where xs' = xs ++ xs'
